DOMAIN

reverse_proxy /tunnel {
  to 127.0.0.1:8080
  header_up X-Real-IP {http.request.remote}
  header_up X-Forwarded-For {http.request.remote}
}

rewrite / /guacamole

reverse_proxy /* {
  to 127.0.0.1:8080
  header_up X-Real-IP {http.request.remote}
  header_up X-Forwarded-For {http.request.remote}
}